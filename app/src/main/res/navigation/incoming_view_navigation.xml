<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/incoming_view_navigation"
    app:startDestination="@id/deliveryFragment">

    <fragment
        android:id="@+id/deliveryFragment"
        android:name="com.example.catnicwarehouse.incoming.deliveries.presentation.fragment.DeliveryFragment"
        android:label="fragment_delivery"
        tools:layout="@layout/fragment_delivery">


        <action
            android:id="@+id/action_deliveryFragment_to_deliveryType"
            app:destination="@id/deliveryType" />
    </fragment>


    <fragment
        android:id="@+id/supplierFragment"
        android:name="com.example.catnicwarehouse.incoming.suppliers.presentation.fragment.SuppliersFragment"
        android:label="fragment_supplier"
        tools:layout="@layout/fragment_supplier">


        <action
            android:id="@+id/action_supplierFragment_to_articlesFragment"
            app:destination="@id/articlesFragment" />
        <action
            android:id="@+id/action_supplierFragment_to_articleSelectionFragment3"
            app:destination="@id/articleSelectionFragment3" />
        <action
            android:id="@+id/action_supplierFragment_to_matchFoundFragment"
            app:destination="@id/matchFoundFragment" />
        <action
            android:id="@+id/action_supplierFragment_to_unloadingStockyardFragment"
            app:destination="@id/unloadingStockyardFragment" />
    </fragment>


    <dialog
        android:id="@+id/deliveryType"
        android:name="com.example.catnicwarehouse.incoming.deliveryType.presentation.bottomSheet.DeliveryTypeBottomSheet"
        android:label="bottomSheetFragment_delivery_type"
        tools:layout="@layout/bottom_sheet_delivery">

        <action
            android:id="@+id/action_deliveryType_to_customersFragment"
            app:destination="@id/customersFragment" />
        <action
            android:id="@+id/action_deliveryType_to_supplierFragment"
            app:destination="@id/supplierFragment" />
    </dialog>


    <fragment
        android:id="@+id/searchSupplierFragment"
        android:name="com.example.catnicwarehouse.main.search_supplier.SearchSupplierFragmentOld"
        android:label="fragment_search_supplier"
        tools:layout="@layout/fragment_search_supplier">
        <action
            android:id="@+id/action_searchSupplierFragment_to_scanFragment"
            app:destination="@id/scanFragment"
            app:popUpTo="@id/searchSupplierFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/scanFragment"
        android:name="com.example.catnicwarehouse.main.scan.ScanFragmentOld"
        android:label="ScanFragment"
        tools:layout="@layout/fragment_scan">
        <action
            android:id="@+id/action_scanFragment_to_manuelInputFragment"
            app:destination="@id/manuelInputFragment" />
        <action
            android:id="@+id/action_scanFragment_to_detailProductFragment"
            app:destination="@id/detailProductFragment" />
        <action
            android:id="@+id/action_scanFragment_to_articleFragment"
            app:destination="@id/articleFragment" />
        <action
            android:id="@+id/action_scanFragment_to_detailProductFragment2"
            app:destination="@id/detailProductFragment" />
    </fragment>
    <fragment
        android:id="@+id/manuelInputFragment"
        android:name="com.example.catnicwarehouse.main.manual_input_code.ManuelInputFragmentOld"
        android:label="ManuelInputFragment"
        tools:layout="@layout/fragment_manual_input">
        <action
            android:id="@+id/action_manuelInputFragment_to_detailProductFragment"
            app:destination="@id/detailProductFragment" />
        <action
            android:id="@+id/action_manuelInputFragment_to_articleFragment"
            app:destination="@id/articleFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailProductFragment"
        android:name="com.example.catnicwarehouse.main.detail_article.DetailArticleFragmentOld"
        android:label="DetailProductFragment"
        tools:layout="@layout/fragment_detail_article">
        <action
            android:id="@+id/action_detailProductFragment_to_amountItemFragment"
            app:destination="@id/amountItemFragmentOld" />
        <action
            android:id="@+id/action_detailProductFragment_to_purchaseOrderFragment"
            app:destination="@id/purchaseOrdersFragment" />
        <action
            android:id="@+id/action_detailProductFragment_to_articleFragment"
            app:destination="@id/articleFragment"
            app:popUpTo="@id/deliveryFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/amountItemFragmentOld"
        android:name="com.example.catnicwarehouse.main.amountItems.AmountItemFragmentOld"
        android:label="fragment_amount_item"
        tools:layout="@layout/fragment_amount_item">
        <action
            android:id="@+id/action_amountItemFragment_to_detailPurchaseOrderFragment"
            app:destination="@id/detailPurchaseOrderFragment"
            app:popUpTo="@+id/amountItemFragmentOld"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/purchaseOrdersFragment"
        android:name="com.example.catnicwarehouse.main.purchase_orders.PurchaseOrdersFragmentOld"
        android:label="PurchaseOrderFragment"
        tools:layout="@layout/fragment_purchase_order">
        <action
            android:id="@+id/action_purchaseOrderFragment_to_amountItemFragment"
            app:destination="@id/amountItemFragmentOld" />
        <action
            android:id="@+id/action_purchaseOrdersFragment_to_detailPurchaseOrderFragment"
            app:destination="@id/detailPurchaseOrderFragment" />
    </fragment>
    <fragment
        android:id="@+id/articleFragment"
        android:name="com.example.catnicwarehouse.main.article.ArticleFragmentOld"
        android:label="ArticleFragment"
        tools:layout="@layout/fragment_article">
        <action
            android:id="@+id/action_articleFragment_to_detailProductFragment"
            app:destination="@id/detailProductFragment" />
        <action
            android:id="@+id/action_articleFragment_to_detailDeliveryFragment"
            app:destination="@id/detailDeliveryFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailPurchaseOrderFragment"
        android:name="com.example.catnicwarehouse.main.detail_purchase_order.DetailPurchaseOrderFragmentOld"
        android:label="DetailPurchaseOrderFragment"
        tools:layout="@layout/fragment_detail_purchase_order">
        <action
            android:id="@+id/action_detailPurchaseOrderFragment_to_amountItemFragment"
            app:destination="@id/amountItemFragmentOld" />
        <action
            android:id="@+id/action_detailPurchaseOrderFragment_to_articleFragment"
            app:destination="@id/articleFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailDeliveryFragment"
        android:name="com.example.catnicwarehouse.main.detail_delivery.DetailDeliveryFragmentOld"
        android:label="DetailDeliveryFragment"
        tools:layout="@layout/fragment_detail_delivery">
        <action
            android:id="@+id/action_detailDeliveryFragment_to_deliveryNoteFragment"
            app:destination="@id/deliveryNoteFragment" />
    </fragment>
    <fragment
        android:id="@+id/deliveryNoteFragment"
        android:name="com.example.catnicwarehouse.main.delivery_note.DeliveryNoteFragment"
        android:label="DeliveryNoteFragment"
        tools:layout="@layout/fragment_delivery_note" />


    <fragment
        android:id="@+id/unloadingStockyardFragment"
        android:name="com.example.catnicwarehouse.incoming.unloadingStockyards.presentation.fragment.UnloadingStockyardsFragment"
        android:label="fragment_unloading_stockyard"
        tools:layout="@layout/fragment_unloading_stockyards">
        <argument
            android:name="scanType"
            android:defaultValue="ONLY_STOCKYARD"
            app:argType="string" />


        <action
            android:id="@+id/action_unloadingStockyardFragment_to_articlesFragment"
            app:destination="@id/articlesFragment" />
        <action
            android:id="@+id/action_unloadingStockyardFragment_to_matchFoundFragment"
            app:destination="@id/matchFoundFragment" />
        <action
            android:id="@+id/action_unloadingStockyardFragment_to_articleSelectionFragment3"
            app:destination="@id/articleSelectionFragment3" />
        <action
            android:id="@+id/action_unloadingStockyardFragment_to_stockyardTreeFragment"
            app:destination="@id/stockyardTreeFragment" />
    </fragment>


    <dialog

        android:id="@+id/manualInputBottomSheet"
        android:name="com.example.catnicwarehouse.scan.presentation.bottomSheetFragment.ManualInputBottomSheet"
        android:label="bottomSheetFragment_delivery_type"
        tools:layout="@layout/bottom_sheet_delivery">

    </dialog>


    <fragment
        android:id="@+id/matchFoundFragment"
        android:name="com.example.catnicwarehouse.incoming.matchFound.presentation.fragment.MatchFoundFragment"
        android:label="fragment_match_found"
        tools:layout="@layout/fragment_match_found">

        <argument
            android:name="deliveryItemId"
            app:argType="string"
            android:defaultValue=""
            />

        <action
            android:id="@+id/action_matchFoundFragment_to_amountItemFragment"
            app:destination="@id/amountItemFragment">
            <argument
                android:name="amount"
                app:argType="string" />
            <argument
                android:name="unit"
                app:argType="string" />
            <argument
                android:name="itemType"
                app:argType="string" />
        </action>
        <action
            android:id="@+id/action_matchFoundFragment_to_commentFragment"
            app:destination="@id/commentFragment" />
        <action
            android:id="@+id/action_matchFoundFragment_to_unloadingStockyardFragment"
            app:destination="@id/unloadingStockyardFragment">
            <argument
                android:name="scanType"
                app:argType="string" />
        </action>
        <action
            android:id="@+id/action_matchFoundFragment_to_articlesFragment"
            app:destination="@id/articlesFragment" />

    </fragment>

    <dialog
        android:id="@+id/defectiveArticleBottomSheetFragment"
        android:name="com.example.catnicwarehouse.incoming.defective.presentation.bottomSheetFragment.DefectiveArticleBottomSheetFragment"
        android:label="bottom_sheet_defective_article"
        tools:layout="@layout/defective_article_bottom_sheet_fragment">
        <action
            android:id="@+id/action_defectiveArticleBottomSheetFragment_to_amountItemFragment"
            app:destination="@id/amountItemFragment"></action>
    </dialog>


    <fragment
        android:id="@+id/amountItemFragment"
        android:name="com.example.catnicwarehouse.incoming.amountItem.presentation.fragment.AmountItemFragment"
        android:label="fragment_amount_item"
        tools:layout="@layout/fragment_amount_item2"></fragment>

    <fragment
        android:id="@+id/commentFragment"
        android:name="com.example.catnicwarehouse.incoming.comment.presentation.fragment.CommentFragment"
        android:label="fragment_comment"
        tools:layout="@layout/fragment_comment"></fragment>

    <fragment
        android:id="@+id/articlesFragment"
        android:name="com.example.catnicwarehouse.incoming.articles.presentation.fragment.ArticlesFragment"
        android:label="fragment_article"
        tools:layout="@layout/fragment_delivery">
        <action
            android:id="@+id/action_articlesFragment_to_deliveryDetailsFragment"
            app:destination="@id/deliveryDetailsFragment" />
        <action
            android:id="@+id/action_articlesFragment_to_unloadingStockyardFragment"
            app:destination="@id/unloadingStockyardFragment">
            <argument
                android:name="scanType"
                app:argType="string" />
        </action>
        <action
            android:id="@+id/action_articlesFragment_to_matchFoundFragment"
            app:destination="@id/matchFoundFragment" >
        <argument
            android:name="deliveryItemId"
            app:argType="string" />
        </action>
        <action
            android:id="@+id/action_articlesFragment_to_unloadingStockyardFragment2"
            app:destination="@id/unloadingStockyardFragment" />
        <action
            android:id="@+id/action_articlesFragment_to_unloadingStockyardFragment3"
            app:destination="@id/unloadingStockyardFragment" />
        <action
            android:id="@+id/action_articlesFragment_to_articleSelectionFragment3"
            app:destination="@id/articleSelectionFragment3" />


    </fragment>

    <fragment
        android:id="@+id/deliveryDetailsFragment"
        android:name="com.example.catnicwarehouse.incoming.deliveryDetail.presentation.fragment.DeliveryDetailsFragment"
        android:label="fragment_article_details"
        tools:layout="@layout/fragment_article_details"></fragment>


    <fragment
        android:id="@+id/scanDeviceInstructionsFragment"
        android:name="com.example.catnicwarehouse.scan.presentation.fragment.ScanDeviceInstructionsFragment"
        android:label="fragment_scan_device_instructions"
        tools:layout="@layout/scan_device_info_layout">
        <argument
            android:name="scanType"
            android:defaultValue="STOCKYARD"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/articleSelectionFragment3"
        android:name="com.example.catnicwarehouse.incoming.articles.presentation.fragment.ArticleSelectionFragment"
        android:label="ArticleSelectionFragment" >
        <action
            android:id="@+id/action_articleSelectionFragment3_to_matchFoundFragment"
            app:destination="@id/matchFoundFragment" />
        <action
            android:id="@+id/action_articleSelectionFragment3_to_unloadingStockyardFragment"
            app:destination="@id/unloadingStockyardFragment" />
    </fragment>
    <fragment
        android:id="@+id/stockyardTreeFragment"
        android:name="com.example.catnicwarehouse.scan.presentation.fragment.StockyardTreeFragment"
        android:label="fragment_stockyard_tree"
        tools:layout="@layout/fragment_stockyard_tree" >
        <argument
            android:name="selectedStockyardId"
            android:defaultValue="0"
            app:argType="string" />

        <argument
            android:name="scanType"
            android:defaultValue="STOCKYARD"
            app:argType="string" />

        <argument
            android:name="moduleType"
            android:defaultValue="Packing"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/customersFragment"
        android:name="com.example.catnicwarehouse.incoming.suppliers.presentation.fragment.CustomersFragment"
        android:label="fragment_customers"
        tools:layout="@layout/fragment_customers" >
        <action
            android:id="@+id/action_customersFragment_to_articleSelectionFragment3"
            app:destination="@id/articleSelectionFragment3" />
        <action
            android:id="@+id/action_customersFragment_to_articlesFragment"
            app:destination="@id/articlesFragment" />
        <action
            android:id="@+id/action_customersFragment_to_matchFoundFragment"
            app:destination="@id/matchFoundFragment" />
        <action
            android:id="@+id/action_customersFragment_to_unloadingStockyardFragment"
            app:destination="@id/unloadingStockyardFragment" />
    </fragment>

</navigation>